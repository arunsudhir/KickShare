extends layout

block content
  h1= title
  p Welcome to #{title}
  #fb-root
  script.
      (function(d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) return;
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.3&appId=1455445644768896"; 
      fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));
  .fb-login-button(data-max-rows='1', data-size='large', data-show-faces='false', data-auto-logout-link='true')
